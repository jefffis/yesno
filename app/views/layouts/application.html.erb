<%
vote_yes = params[:yes]
vote_no = params[:no]
%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=Edge;chrome=1">
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title><% if @q %><%= @q.title %> | <% end %>Yesno</title>
		<link href='http://fonts.googleapis.com/css?family=Archivo+Narrow:400,700|Old+Standard+TT:400,700,400italic' rel='stylesheet' type='text/css' />
		<%= stylesheet_link_tag "application", :media => "all" %>
		<%= csrf_meta_tags %>
		<!--<link rel="apple-touch-icon-precomposed" href="/assets/icon.png" />
		<link rel="shortcut icon" href="/assets/favicon.png" type="image/x-icon" />
		<meta name="robots" content="noodp,noydir" />
		<link rel="robots" href="robots.txt" />-->
</head>
<body>
	<header>
		<h2><%= link_to('/') do %>
			<strong data-vote="yes"<% if vote_yes %> class="active"<% end %>>yes <span class="icon-thumbs-up"></span></strong>
			<em>:</em>
			<strong data-vote="no"<% if vote_no %> class="active"<% end %>>no <span class="icon-thumbs-down"></span></strong>
		<% end %></h2>
		<!--<img src="data:image/svg+xml;base64," />-->
	</header>
	<div class="main" role="main">
		<%= yield %>
	</div>
	<%= javascript_include_tag "application" %>
</body>
</html>
