<%
vote_yes = params[:yes]
vote_no = params[:no]
pop = params[:pop]
url = request.fullpath.split('?')
%>
<%#= url[0].split('/')[1] %>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=Edge;chrome=1">
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title><% if @q && url[0].split('/')[2] != 'new' %><%= @q.title %> | <% end %><% if url[0].split('/')[2] == 'new' %>Add a new question | <% end %>yesno<% if url[0] == '/' %> | Simple yes/no questions for settling big debates.<% end %></title>
		<!--<link href='http://fonts.googleapis.com/css?family=Carrois+Gothic|Varela+Round' rel='stylesheet' type='text/css' />-->
		<%= stylesheet_link_tag "application", :media => "all" %>
		<%= csrf_meta_tags %>
		<link rel="apple-touch-icon-precomposed" href="/assets/yesno-icon.png" />
		<link rel="shortcut icon" href="/assets/favicon.png" type="image/x-icon" />
		<!--<meta name="robots" content="noodp,noydir" />
		<link rel="robots" href="robots.txt" />-->
</head>
<% if url[0].split('/')[1] != 'qs' %>
<% q_total = @qs.length %>
<% end %>
<body class="<% if url[0] == '/' %> home<% else %> inner<% end %><% if url[0].split('/')[1] != 'qs' && q_total < 7 %> short<% end %>">
	<% if !cookies[:welcome] %>
	<% if url[0] == '/' %>
		<div class="welcome<% if vote_no %> blue<% end %>" id="welcome">
			<div>
				<h2>
					<img src="/assets/yes.svg" class="yes" />
					<img src="/assets/no.svg" class="no" />
				</h2>
			</div>
		</div>
	<% end %>
	<% end %>
	<div class="wrapper">
		<header id="v">
			<!--<h2><%= link_to('/') do %>
				<strong data-vote="yes"<% if vote_yes %> class="active"<% end %>>yes <span class="icon-thumbs-up"></span></strong>
				<em>:</em>
				<strong data-vote="no"<% if vote_no %> class="active"<% end %>>no <span class="icon-thumbs-down"></span></strong>
			<% end %></h2>-->
			<% if url[0] == '/' || url[0].split('/')[1] == 'q' %><div class="about" id="info-show"><span class="icon-bolt"></span> <em>about</em></div>
			<div class="popular" id="popular<% if pop %>-rmv<% end %>"><span class="icon-<% if pop %>remove<% else %>star n<% end %>"></span> <em>popular</em></div><% end %>
			<div class="info" id="info">
				<div>
					<strong>yesno</strong> is a simple Ruby on Rails app that allows users (without logging in) to vote on yes or no questions. They can also create their own and share them socailly or with friends. Enjoy y&rsquo;all.
					<a href="http://jefff.co" target="_blank"><span>made with love by</span> jefff</a>
				</div>
			</div>
			<h2><%
				# if either filter is active
				if vote_no || vote_yes %>
				<% if vote_no %>
				<img class="yes" id="yes-img" src="data:image/svg+xml;base64," />
				<img src="data:image/svg+xml;base64," class="no" id="no-img" />
				<% else %>
				<img src="data:image/svg+xml;base64," class="yes" id="yes-img" />
				<img class="no" id="no-img" src="data:image/svg+xml;base64," />
				<% end %>
				<%
				# or if no selection, show normal for both
				else %>
				<img src="data:image/svg+xml;base64," class="yes" id="yes-img" />
				<img src="data:image/svg+xml;base64," class="no" id="no-img" />
				<% end %>
			</h2>
			<% if vote_yes || vote_no || pop %>
				<div class="center sm" id="remove-params" data-url="<%= request.fullpath.split('?')[0] %>"><% if !pop %><% if vote_no %>yes<% elsif vote_yes %>no<% end %> questions filtered out<% else %>viewing popular questions<% end %> <span class="icon-remove"></span></div><% end %>
		</header>
		<div class="main" role="main">
			<%= yield %>
		</div>
	</div>
	<%= javascript_include_tag "application" %>
	<% if notice %>
	<script>
	SetCookie('<%= @q.unique_id %>-author','yes');
	</script>
	<% end %>
	<% if !cookies[:welcome] %>
	<script>
		SetCookie('welcome',1);
		setTimeout(function(){
			$('#welcome').fadeOut(700);
		}, 700);
	</script>
	<% end %>
</body>
</html>
